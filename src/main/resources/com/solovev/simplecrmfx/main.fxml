<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.input.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="307.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.solovev.simplecrmfx.controllers.MainController"
            stylesheets="@bootstrap3.css">
    <children>
        <TableView fx:id="tableView" layoutX="7.0" layoutY="33.0" prefHeight="357.0" prefWidth="294.0">
        </TableView>
        <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="307.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#menuItemOpen" text="Open">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="O" control="DOWN" meta="UP" shift="UP"
                                                    shortcut="UP"/>
                            </accelerator>
                        </MenuItem>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
